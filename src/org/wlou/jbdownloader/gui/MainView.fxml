<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="CENTER" hgap="10.0" prefWidth="455.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/2.2" fx:controller="org.wlou.jbdownloader.gui.MainController">
  <Label text="Downloads List" GridPane.rowIndex="0"/>
  <Button onAction="#addNewDownload" text="Add new task" textAlignment="LEFT" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
    <GridPane.margin>
      <Insets right="10.0" />
    </GridPane.margin>
  </Button>
  <TableView fx:id="tasksTableView" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2" GridPane.vgrow="ALWAYS">
    <columns>
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="121.0" text="Url" fx:id="srcUrlColumn" />
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="93.0" text="Progress" fx:id="progressColumn" />
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="117.0" text="Information" fx:id="informationColumn" />
      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="121.0" text="Result File" fx:id="targetFileColumn" />
    </columns>
    <GridPane.margin>
      <Insets bottom="10.0" left="10.0" right="10.0" />
    </GridPane.margin>
  </TableView>
  <rowConstraints>
    <RowConstraints fillHeight="true" maxHeight="34.0" prefHeight="34.0" />
    <RowConstraints maxHeight="21.0" prefHeight="21.0" />
    <RowConstraints maxHeight="410.0" prefHeight="410.0" />
  </rowConstraints>
</GridPane>
